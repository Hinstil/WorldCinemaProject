<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#150D0B">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logotype"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="47dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button5"
        android:layout_width="330dp"
        android:layout_height="47dp"
        android:background="#DB43DE"
        android:text="Регистрация"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="565dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="330dp"
        android:layout_height="45dp"
        android:background="@drawable/back"
        android:ems="10"
        android:inputType="textPersonName"
        android:padding="10dp"
        android:text="Имя"
        android:textColor="#AFAFAF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="254dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="330dp"
        android:layout_height="45dp"
        android:background="@drawable/back"
        android:ems="10"
        android:inputType="textPersonName"
        android:padding="10dp"
        android:text="Фамилия"
        android:textColor="#AFAFAF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="312dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress3"
        android:layout_width="330dp"
        android:layout_height="45dp"
        android:background="@drawable/back"
        android:backgroundTint="@drawable/back"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:padding="10dp"
        android:scrollbarThumbHorizontal="#ffffff"
        android:scrollbarThumbVertical="#ffffff"
        android:selectAllOnFocus="false"
        android:singleLine="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#AFAFAF"
        android:textColorHint="#FFFFFF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="374dp"
        tools:text="E-mail" />

    <EditText
        android:id="@+id/editTextTextPassword3"
        android:layout_width="330dp"
        android:layout_height="45dp"
        android:background="@drawable/back"
        android:ems="10"
        android:padding="10dp"
        android:text="Пароль"
        android:textColor="#AFAFAF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="436dp" />

    <EditText
        android:id="@+id/editTextTextPassword4"
        android:layout_width="330dp"
        android:layout_height="45dp"
        android:background="@drawable/back"
        android:ems="10"
        android:padding="10dp"
        android:text="Пароль"
        android:textColor="#AFAFAF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="493dp" />

    <Button
        android:id="@+id/button6"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="330dp"
        android:layout_height="47dp"
        android:background="@drawable/back"
        android:backgroundTint="@drawable/back"
        android:outlineAmbientShadowColor="#FFFFFF"
        android:outlineProvider="none"
        android:outlineSpotShadowColor="#FFFFFF"
        android:text="У меня уже есть аккаунт"
        android:textAlignment="center"
        android:textColor="#DB43DE"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="639dp" />

</androidx.constraintlayout.widget.ConstraintLayout>